{
    "name": "tic-tac-toe",
    "version": "1.0.0",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "typecheck": "tsc",
        "format:check": "npx prettier --check \"**/*.{js,json,ts,tsx}\"",
        "format": "npx prettier --write \"**/*.{js,json,ts,tsx}\"",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run typecheck && lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": "eslint",
        "**/*.{js,json,ts,tsx}": "prettier --write"
    },
    "dependencies": {
        "expo": "~47.0.12",
        "expo-status-bar": "~1.4.2",
        "husky-4": "^4.3.8",
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "react-native": "0.70.5",
        "react-native-web": "~0.18.9"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@types/react": "~18.0.14",
        "@types/react-native": "~0.70.6",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-react": "^7.32.1",
        "lint-staged": "^13.1.0",
        "prettier": "2.8.3",
        "typescript": "^4.6.3"
    },
    "private": true
}
